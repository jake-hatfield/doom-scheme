{
	"extends": "@tsconfig/react-native/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"*": ["*"],
			"tests": ["tests/*"],
			"@assets/*": ["src/assets/*"],
			"@components/*": ["src/lib/components/*"],
			"@context/*": ["src/context/*"],
			"@env": [".env"],
			"@lib/*": ["src/lib/*"],
			"@screens/*": ["src/screens/*"],
			"@stacks/*": ["src/stacks/*"],
			"$types/*": ["src/types/*"],
			"@utils/*": ["src/lib/utilities/*"]
		}
	}
}
